    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bs-platform": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "^3.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^2.2.3"
